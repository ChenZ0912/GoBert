{
  "manifest_version": 2,
  "name": "Hello World",
  "version": "1.0",
  "key": "",
  "description": "Hello world extension for senior design",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "background": {
    "scripts": ["background.js"],
    "js": ["jquery.min.js", "content.js"]
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["*://www.gobertweb.com/*"],
    "js": ["jquery.min.js", "content.js"]
  }],
  "permissions": [
    "tabs",
    "contextMenus",
    "storage",
    "*://www.gobertweb.com/*"
  ],
  "externally_connectable": {
    "matches": [
      "*://www.gobertweb.com/*"
    ]
  },

  "short_name": "Web App",
  "start_url": "/",
  "display": "standalone",
  "theme_color": "#000000",
  "background_color": "#ffffff"
}